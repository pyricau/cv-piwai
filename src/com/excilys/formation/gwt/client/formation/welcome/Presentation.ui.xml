<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">

	<ui:image field="octocat" src="octocat_social.png" />

	<ui:style>
		@sprite .octocat {
			gwt-image: 'octocat';
			margin-right: 20px;
			float:left;
		}
		.space {
			margin-top: 40px;
		}
	</ui:style>

	<div>
		<header>Steal this slideshow!</header>
		<section>
			<div class="{style.space}">
			<h1>Slides full GWT</h1>
			<p>Basé sur <a href="http://gwthtml5.appspot.com">http://gwthtml5.appspot.com</a></p>
			</div>
			<div class="{style.space}">
			<h1>Hébergé sur AppEngine</h1>
			<p>Par ici : <a href="http://excilys-gwt-slides.appspot.com">http://excilys-gwt-slides.appspot.com</a></p>
			</div>
			<div class="{style.space}">
			<h1>Forkez moi sur Github!</h1>
			<div class="{style.octocat}" />
			<p>
			<a href="https://github.com/pyricau/slides-formation-gwt">https://github.com/pyricau/slides-formation-gwt</a>
			</p>
			</div>

		</section>
	</div>
</ui:UiBinder>